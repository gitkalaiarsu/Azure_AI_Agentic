{
  "name": "cx-assistant",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.13",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "moment": "^2.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.0.2",
    "react-scripts": "^5.0.1",
    "react-scrollable-feed": "^1.3.2",
    "react-toastify": "^10.0.4",
    "redux": "^5.0.0",
    "socket.io-client": "^4.6.1",
    "sweetalert2": "^11.10.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_ENV=staging GENERATE_SOURCEMAP=false react-scripts build",
    "build:dev": "REACT_APP_ENV=staging GENERATE_SOURCEMAP=false react-scripts build",
    "build:stage": "REACT_APP_ENV=staging GENERATE_SOURCEMAP=false react-scripts build",
    "build:prod": "REACT_APP_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "prepare": "cd .. && npx husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "3.1.1",
    "react-app-rewired": "^2.2.1",
    "style-loader": "^3.3.3"
  },
  "lint-staged": {
    "*.{js,css,md,html,json}": "npm run format"
  }
}
